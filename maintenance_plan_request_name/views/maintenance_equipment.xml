<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_order_form" model="ir.ui.view">
        <field name="name">maintenance_equipment_request_name.view_order_form</field>
        <field name="model">maintenance.equipment</field>
        <field name="inherit_id" ref="maintenance_equipment.maintenance_plan_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']/tree[1]/field[@name='sequence']"
                position="after">
                <field name="position" />
            </xpath>
        </field>
    </record>

    <record id="view_tree" model="ir.ui.view">
        <field name="name">maintenance_equipment_request_name.view_tree</field>
        <field name="model">maintenance_plan_request_name</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="value" />
            </tree>
        </field>
    </record>

</odoo>
